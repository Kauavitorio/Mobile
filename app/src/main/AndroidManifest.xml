        <activity android:name=".GerenciarUserActivity" />
        <activity android:name=".PerfilUsuarioActivity" />
        <activity android:name=".NossaEquipeActivity" />
        <activity android:name=".PrincipalActivity" />
        <activity android:name=".AvisoErroActivity" />
        <activity android:name=".MenuEsqueciASenhaActivity" />
        <activity android:name=".LoadingActivity" />
        <activity android:name=".CriarContaActivity" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>